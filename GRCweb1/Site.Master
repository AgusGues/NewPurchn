            function getSPPInfo() {
                var url = '<%=ResolveUrl("~/SearchAll.ashx") %>';
                $.post(url, { 'id': '3', 'Status': '' }, function (response) {
                    $('#usrmsg').val('');
                    if (response != '') {
                        $('#msgSPP').attr('visible', 'true');
                        $('#msgSPP').html(response);
                    } else {
                        $('#msgSPP').attr('visible', 'true');
                    }
                });
            } /**/


            function getPoInfo() {
                var url = '<%=ResolveUrl("~/SearchAll.ashx") %>';
                $.post(url, { 'id': '4', 'Status': '' }, function (response) {
                    $('#usrmsg').val('');
                    if (response != '') {
                        $('#msgSPP').attr('visible', 'true');
                        $('#msgSPP').html(response);
                    } else {
                        $('#msgSPP').attr('visible', 'true');
                    }
                });
            } /**/
            function getTaskInfo() {
                var url = '<%=ResolveUrl("~/SearchAll.ashx") %>';
                $.post(url, { 'id': '6', 'Approval': '' }, function (response) {
                    var d = JSON.parse(response);
                    if (response != '[]') {
                        $('#usrmsg').val('');
                        $('#msgSPP').html(d[0].TaskName);
                    }


                });
                $.ajax({
                    type: 'POST',
                    url: url,
                    data: { id: '6' },
                    dataType: 'json',
                    success: function (datax) {
                        if (datax.length > 0) {

                            var title = '=>' + datax[0].PIC + ' - ' + datax[0].TaskName + '\n\r';
                            $('#tsInfo')
                                .html(datax.length + " Task harus di update")
                                .attr("title", title);

                        }
                    }
                });
            }

            function getWoInfo() {
                var url = '<%=ResolveUrl("~/SearchAll.ashx") %>';
                $.post(url, { 'id': '8', 'Status': '' }, function (response) {
